%tbody
	- jobs_from_params.each do |job|
		%tr
			%td
				= job.title
				- job.goals.each do |goal|
					= link_to goal.name, "/goals/#{goal.name}", class: "goallabel #{goal.color}"

			%td
				- job.organizations.each do |org|
					= "#{org.name}"
			%td= job.employment_type
			%td= job.date_posted.strftime('%b %d, %Y')